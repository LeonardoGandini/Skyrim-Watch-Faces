<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.watch.lgandini.com.skyrimwatchfaces">

    <uses-sdk android:minSdkVersion="21" android:targetSdkVersion="21"/>

    <uses-feature android:name="android.hardware.type.watch"/>

    <uses-permission android:name="com.google.android.permission.PROVIDE_BACKGROUND"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/ic_launcher"
        android:theme="@android:style/Theme.DeviceDefault">

        <service
            android:name="AnalogDovahkiin"
            android:allowEmbedded="true"
            android:label="@string/dovahkiin_name"
            android:permission="android.permission.BIND_WALLPAPER"
            android:taskAffinity="">

            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/watch_face"/>
            <meta-data
                android:name="com.google.android.wearable.watchface.preview"
                android:resource="@drawable/preview_dovahkiin"/>
            <meta-data
                android:name="com.google.android.clockwork.home.preview_circular"
                android:resource="@drawable/preview_dovahkiin"/>
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
                <category android:name="com.google.android.wearable.watchface.category.WATCH_FACE"/>
            </intent-filter>
        </service>
        <service
            android:name="AnalogWeKnow"
            android:allowEmbedded="true"
            android:label="@string/weknow_name"
            android:permission="android.permission.BIND_WALLPAPER"
            android:taskAffinity="">

            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/watch_face"/>
            <meta-data
                android:name="com.google.android.wearable.watchface.preview"
                android:resource="@drawable/preview_weknow"/>
            <meta-data
                android:name="com.google.android.clockwork.home.preview_circular"
                android:resource="@drawable/preview_weknow"/>
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
                <category android:name="com.google.android.wearable.watchface.category.WATCH_FACE"/>
            </intent-filter>
        </service>
        <service
            android:name="AnalogFight"
            android:allowEmbedded="true"
            android:label="@string/fight_name"
            android:permission="android.permission.BIND_WALLPAPER"
            android:taskAffinity="">

            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/watch_face"/>
            <meta-data
                android:name="com.google.android.wearable.watchface.preview"
                android:resource="@drawable/preview_fight"/>
            <meta-data
                android:name="com.google.android.clockwork.home.preview_circular"
                android:resource="@drawable/preview_fight"/>
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
                <category android:name="com.google.android.wearable.watchface.category.WATCH_FACE"/>
            </intent-filter>
        </service>
        <service
            android:name="AnalogSunset"
            android:allowEmbedded="true"
            android:label="@string/sunset_name"
            android:permission="android.permission.BIND_WALLPAPER"
            android:taskAffinity="">

            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/watch_face"/>
            <meta-data
                android:name="com.google.android.wearable.watchface.preview"
                android:resource="@drawable/preview_calmsunset"/>
            <meta-data
                android:name="com.google.android.clockwork.home.preview_circular"
                android:resource="@drawable/preview_calmsunset"/>
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
                <category android:name="com.google.android.wearable.watchface.category.WATCH_FACE"/>
            </intent-filter>
        </service>
        <service
            android:name="AnalogSkyrim"
            android:allowEmbedded="true"
            android:label="@string/skyrim_name"
            android:permission="android.permission.BIND_WALLPAPER"
            android:taskAffinity="">

            <meta-data
                android:name="android.service.wallpaper"
                android:resource="@xml/watch_face"/>
            <meta-data
                android:name="com.google.android.wearable.watchface.preview"
                android:resource="@drawable/preview_skyrim"/>
            <meta-data
                android:name="com.google.android.clockwork.home.preview_circular"
                android:resource="@drawable/preview_skyrim"/>
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
                <category android:name="com.google.android.wearable.watchface.category.WATCH_FACE"/>
            </intent-filter>
        </service>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>

    </application>

</manifest>